
<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net8.0-windows</TargetFramework>
		<!-- or net7/6 -->
		<UseWPF>true</UseWPF>
		<UseWindowsForms>true</UseWindowsForms>
		<!-- IMPORTANT -->
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="custom_icon.png" />
	  <None Remove="custom_icon2.png" />
	  <None Remove="ScreenRecorderTray.custom_icon.png" />
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Include="custom_icon.png" />
	  <EmbeddedResource Include="custom_icon2.png" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="NAudio" Version="2.2.1" />
	</ItemGroup>

</Project>
